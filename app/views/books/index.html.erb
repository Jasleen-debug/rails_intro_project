
<h2 class="text-center p-4"> Top GoodReads Books </h2>
<%= paginate @books %>
<ol class="list-group list-group-numbered p-4">
<% @books.each do | book | %>
<li class="list-group-item"><%= link_to book.title, book %> was published by <%= link_to book.publisher.name, book.publisher %> </li>
<% end %>
</ol>
<%= paginate @books %>